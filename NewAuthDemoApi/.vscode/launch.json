{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch .NET API",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/bin/Debug/net8.0/AuthDemoApi.dll",
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
